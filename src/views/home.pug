doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Masked Wolf Playlist
  body
    h1 Masked Wolf Playlist
    div#playlist
      each track in tracks
        div
          h3= track.name
          p Play count: #{track.playcount}
          p Listeners: #{track.listeners}
