extends base
include mixin/music_card

block content
  #section
    .section__header
      span.section__update-time 오늘 23:00 기준
      span.section__title TOP Tracks
      div.show-all-btn
        span show all
    .section__body
      each track, index in trackDetails
        +musicCard(index + 1, track.trackTitle, track.artist, track.albumImage, track.youtubeVideoId)
